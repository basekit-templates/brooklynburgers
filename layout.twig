<!doctype html>
<html lang="{{ page.seoLang }}">
	
		{% include "head.twig" %}
		
		{% include basekit.headScript %}

	<body class="{{ page.backgroundClass }}">

		{% include "header.twig" %}

		<nav class="nav page-wrap">
			{{widget('extendednavigation', 'navigationheader', {})|raw}}
		</nav>
		<div class="header-image page-wrap">
      		{{widget('feature', 'featurehome', {"header": "profile", "buttonText":"", "defaultBgImg": asset('images/feature-bg.jpg'), 'positionTopPercentage': '50', 'showBtn': true})|raw}}
		</div>

		<div class="page-wrap">

			{% block content %}
			{% endblock %}

		</div>

		{% include basekit.bodyScript %}

		<script type="text/javascript">
			!function() {
				function supportsSVG() {
					return !!document.createElementNS && !! document.createElementNS('http://www.w3.org/2000/svg', 'svg').createSVGRect
				}
				if (supportsSVG()) document.documentElement.className += ' svg'
			}()
		</script>

	</body>
</html>